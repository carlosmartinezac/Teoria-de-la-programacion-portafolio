Algoritmo ejemplo 1
	//Variables
	Definir dinero Como real;
	Definir costo Como real;
	Definir cambio Como Real;
	
	//Datos de entrada
	Escribir "INGRESE SU CANTIDAD DE DINERO";
	Leer dinero;
	Escribir "INGRESE EL PRECIO DEL PRODUCTO";
	Leer costo;
	// Proceso 
	cambio = dinero - costo;
	//Datos de salida
	Escribir "Su cambio es: ",cambio;
FinAlgoritmo

